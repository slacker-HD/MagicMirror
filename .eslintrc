{
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "extends": "eslint-config-airbnb",
  "env": {
    "node": true,
    "es6": true,
    "browser": true,
  },
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [2, {
      "ignore": ["electron"]
    }],
    "linebreak-style": 0,
    "strict": 0,
    "no-console": "off",
    "max-len":"off",
    "eol-last":"off",
    "indent":"off"
  }
}